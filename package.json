{
  "name": "wifi-password-finder",
  "version": "0.0.0",
  "private": true,
  "main": "electron/dist/electron/main.js",
  "scripts": {
    "dev": "set VITE_PORT=5179 && vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "electron:compile": "npx tsc -b -w electron",
    "electron:win:start": "chcp 65001 && set NODE_ENV=development && set VITE_PORT=5179 && electron ./electron/dist/electron/main.js",
    "electron:mac:start": "export NODE_ENV=development && export VITE_PORT=5179 && electron ./electron/dist/electron/main.js",
    "app:dir": "electron-builder --dir ./electron-builder.yml",
    "app:dist": "electron-builder --config ./electron-builder.yml"
  },
  "dependencies": {
    "@phosphor-icons/web": "^2.0.3",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@tsconfig/node18": "^18.2.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.17.5",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.15",
    "electron": "^26.1.0",
    "eslint": "^8.46.0",
    "eslint-plugin-vue": "^9.16.1",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.29",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.1.6",
    "vite": "^4.4.9",
    "vitest": "^0.34.2",
    "vue-tsc": "^1.8.8"
  }
}